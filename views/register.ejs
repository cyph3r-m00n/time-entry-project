<form id="loginform" action="/register" class="form-signin" method="POST">
    <img id="logoimg" class="mb-4" src="https://i.ibb.co/WyNjT67/Fox-Wyndrum-Logo-Small.jpg" alt="" >
    <h1 class="h3 mb-3 font-weight-light">Create your account</h1>
    <input type="text" id="inputEmail" name="username" class="form-control" placeholder="Username" required autofocus>
    <input type="password" id="inputPassword" name="password" class="form-control" placeholder="Password" required>
    <button class="btn btn-lg btn-primary btn-block" type="submit">Sign in</button>
    <p class="mt-5 mb-3 text-muted">&copy; 2018-2020</p>
    <p>Already have an account? <a href="/login">Click Here</a></p>
  </form>
